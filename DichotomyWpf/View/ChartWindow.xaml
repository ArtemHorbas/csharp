<Window x:Class="DichotomyWpf.View.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DichotomyWpf.View"
        xmlns:wf="clr-namespace:System.Windows.Forms.DataVisualization.Charting;assembly=System.Windows.Forms.DataVisualization"
        mc:Ignorable="d"
        Title="Graph" Height="700" Width="1000">
    <Grid>
        <WindowsFormsHost>
            <wf:Chart x:Name="chart">
                <wf:Chart.Series>
                    <wf:Series ChartType="Line"  />
                </wf:Chart.Series>
                <wf:Chart.ChartAreas>
                    <wf:ChartArea />
                </wf:Chart.ChartAreas>
            </wf:Chart>
        </WindowsFormsHost>
    </Grid>
</Window>
